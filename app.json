{
  "expo": {
    "name": "Sellit",
    "slug": "sell-it",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.anurag.sellit",
      "versionCode": 1
    },
    "description": "E-Store to sell goods",
    "notification": {
      "iosDisplayInForeground": true
    },
    "githubUrl": "https://github.com/anurag-deore/doneWithIt-ReactNative-App.git"
  }
}
